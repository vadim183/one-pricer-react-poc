{
    "extends": [ "tslint:recommended", "tslint-react", "tslint-config-prettier" ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    },
    "rules": {
        "interface-name": false,
        "ordered-imports": false,
        "jsx-no-lambda": false,
        "jsx-no-bind": false,
        "prefer-const": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "interface-over-type-literal": false,
        "no-console": false,
        "no-empty": false
    }
}